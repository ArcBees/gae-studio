<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <ui:with type="com.arcbees.gaestudio.client.place.NameTokens" field="nameTokens" />

    <ui:style>
        .linkList {
            float: right;
        }

        .link {
            padding-right: 4px;
        }
    </ui:style>

    <g:HTMLPanel>
        <div class="{style.linkList}">
            <g:InlineHyperlink targetHistoryToken="{nameTokens.getVisualizer}" styleName="{style.link}">
                Visualizer
            </g:InlineHyperlink>
            <g:InlineHyperlink targetHistoryToken="{nameTokens.getProfiler}" styleName="{style.link}">
                Profiler
            </g:InlineHyperlink>
        </div>
    </g:HTMLPanel>

</ui:UiBinder>
