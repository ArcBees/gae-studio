<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:with field="visualizer" type="com.arcbees.gaestudio.client.resources.VisualizerResources"/>
    <g:HTMLPanel ui:field="container" styleName="{visualizer.styles.ui}">
        <g:HTML ui:field="jsonContent" styleName="{visualizer.styles.content}"/>
        <g:Label ui:field="more" styleName="{visualizer.styles.dots}" text="[...]"/>
    </g:HTMLPanel>
</ui:UiBinder>
