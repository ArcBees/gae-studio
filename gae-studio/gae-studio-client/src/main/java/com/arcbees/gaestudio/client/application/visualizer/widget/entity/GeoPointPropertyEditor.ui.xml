<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
        xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:with field="res" type="com.arcbees.gaestudio.client.resources.AppResources"/>

    <ui:style>
        .label {
            display: inline-block;
            width: 60px;
            text-align: right;
        }
    </ui:style>

    <g:HTMLPanel styleName="{res.styles.embeddedEntityProperties}">
        <div>
            <g:Label text="Latitude" styleName="{style.label}"/>
            <g:DoubleBox ui:field="latitude"/>
        </div>

        <div>
            <g:Label text="Longitude" styleName="{style.label}"/>
            <g:DoubleBox ui:field="longitude"/>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>
