<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
        xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:with field="res" type="com.arcbees.gaestudio.client.resources.AppResources"/>

    <g:HTMLPanel styleName="{res.styles.embeddedEntityProperties}">
        <g:Label styleName="{res.styles.editorLabel}">Email</g:Label>
        <g:TextBox ui:field="email"/>

        <g:Label styleName="{res.styles.editorLabel}">Auth Domain</g:Label>
        <g:TextBox ui:field="authDomain"/>

        <g:Label styleName="{res.styles.editorLabel}">User ID</g:Label>
        <g:TextBox ui:field="userId"/>

        <g:Label styleName="{res.styles.editorLabel}">Federated Identity</g:Label>
        <g:TextBox ui:field="federatedIdentity"/>
    </g:HTMLPanel>
</ui:UiBinder>
