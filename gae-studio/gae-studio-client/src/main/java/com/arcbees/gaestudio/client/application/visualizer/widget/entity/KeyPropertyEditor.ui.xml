<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:prop='urn:import:com.arcbees.gaestudio.client.application.visualizer.widget.entity'>
    <ui:with field="res" type="com.arcbees.gaestudio.client.resources.AppResources"/>

    <g:HTMLPanel styleName="{res.styles.embeddedEntityProperties}">
        <g:Label>Kind</g:Label>
        <g:TextBox ui:field="kind"/>

        <g:Label>Id</g:Label>
        <g:LongBox ui:field="id"/>

        <g:Label>App Id</g:Label>
        <g:TextBox ui:field="appId"/>

        <g:Label>Name</g:Label>
        <g:TextBox ui:field="name"/>

        <g:Label>Namespace</g:Label>
        <g:TextBox ui:field="namespace"/>

        <g:Label>Namespace's App Id</g:Label>
        <g:TextBox ui:field="appIdNamespace"/>

        <prop:RawPropertyEditor ui:field="parentKey"/>
    </g:HTMLPanel>
</ui:UiBinder>
