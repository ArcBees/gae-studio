<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:with field="res" type="com.arcbees.gaestudio.client.resources.AppResources"/>
    <ui:with field="debugIds" type="com.arcbees.gaestudio.client.debug.DebugIds"/>
    <g:DockLayoutPanel unit="PX" debugId="{debugIds.getApplicationRoot}">
        <g:north size="51">
            <g:HTMLPanel>
                <g:SimplePanel ui:field="messages"/>
                <g:SimplePanel ui:field="header"/>
            </g:HTMLPanel>
        </g:north>
        <g:center styleName="{res.styles.noOverflow}">
            <g:SimpleLayoutPanel ui:field="main" styleName="{res.styles.noOverflow}"/>
        </g:center>
    </g:DockLayoutPanel>
</ui:UiBinder>
