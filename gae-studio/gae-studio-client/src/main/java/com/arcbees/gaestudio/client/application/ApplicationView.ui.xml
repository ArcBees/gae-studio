<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:with field="res" type="com.arcbees.gaestudio.client.resources.AppResources"/>
    <ui:with field="debugIds" type="com.arcbees.gaestudio.client.debug.DebugIds"/>
    <g:HTMLPanel debugId="{debugIds.getApplicationRoot}" ui:field="root">
        <g:HTMLPanel addStyleNames="{res.styles.northSection}" ui:field="northSection">
            <g:SimplePanel ui:field="version"/>
            <g:SimplePanel ui:field="messages"/>
            <g:SimplePanel ui:field="header"/>
        </g:HTMLPanel>
        <g:SimplePanel ui:field="main" styleName="{res.styles.main}"/>
    </g:HTMLPanel>
</ui:UiBinder>
