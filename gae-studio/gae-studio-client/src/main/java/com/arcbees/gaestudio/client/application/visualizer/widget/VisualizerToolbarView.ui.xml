<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:with field="resources" type="com.arcbees.gaestudio.client.resources.AppResources"/>
    <ui:style>
        .buttons {
            background: #2d2d2d;
            height: 100%;
        }
    </ui:style>
    <g:HTMLPanel styleName="{resources.styles.toolbar}">
        <g:HTMLPanel styleName="{style.buttons}" ui:field="buttons" visible="false"/>
        <div class="{resources.styles.clear}"/>
    </g:HTMLPanel>
</ui:UiBinder>
