<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
        xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:style>
        .label {
            text-transform: uppercase;
        }
    </ui:style>
    <g:HTMLPanel>
        <g:Label ui:field="key" styleName="{style.label}"/>
        <g:SimplePanel ui:field="form"/>
    </g:HTMLPanel>
</ui:UiBinder>
