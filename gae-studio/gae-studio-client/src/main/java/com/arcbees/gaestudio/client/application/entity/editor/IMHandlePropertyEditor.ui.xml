<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
        xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <ui:with field="res" type="com.arcbees.gaestudio.client.resources.AppResources"/>

    <g:HTMLPanel styleName="{res.styles.embeddedEntityProperties}">
        <g:Label styleName="{res.styles.editorLabel}">Protocol</g:Label>
        <g:TextBox ui:field="protocol"
                title="The protocol value is either a standard IM scheme (sip, unknown or xmpp) or a URL identifying the IM network for the protocol (e.g. http://aim.com/)."/>
        <g:Label styleName="{res.styles.editorLabel}">Address</g:Label>
        <g:TextBox ui:field="address"/>
    </g:HTMLPanel>
</ui:UiBinder>
