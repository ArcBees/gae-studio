<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <ui:with field="res" type="com.arcbees.gaestudio.client.resources.AppResources"/>
    <ui:with field="entityRes" type="com.arcbees.gaestudio.client.resources.EntityResources"/>

    <g:HTMLPanel styleName="{res.styles.entityDetailPanel}" ui:field="panelRoot">
        <div class="{res.styles.entityDetailPanelVisibility}">
            <div class="{entityRes.styles.fullscreen} {res.styles.expand}" title="View in full screen" ui:field="fullscreen"/>
            <g:SimplePanel ui:field="root" addStyleNames="{entityRes.styles.root} {res.styles.secondTable} {res.styles.entityPanelTransitions}"/>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>
