<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:dd='urn:import:com.arcbees.gaestudio.client.application.widget.dropdown'>
    <ui:with field="res" type="com.arcbees.gaestudio.client.resources.AppResources"/>
    <ui:with field="visualizer" type="com.arcbees.gaestudio.client.resources.VisualizerResources"/>
    <g:HTMLPanel styleName="{visualizer.styles.namespace} {visualizer.styles.root}">
        <dd:Dropdown ui:field="dropdown"/>
        <div ui:field="delete" class="{visualizer.styles.delete} {res.styles.trashSmall}"/>
    </g:HTMLPanel>
</ui:UiBinder>
