<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <ui:with field="res" type="com.arcbees.gaestudio.client.resources.AppResources"/>
    <ui:with field="ent" type="com.arcbees.gaestudio.client.resources.EntityResources"/>

    <g:HTMLPanel styleName="{res.styles.editorMain}">
        <g:Label ui:field="key" styleName="{res.styles.editorLabel}"/>
        <div class="{res.styles.editorLabelSeparator}"/>
        <g:SimplePanel ui:field="form" styleName="{ent.styles.form}"/>
    </g:HTMLPanel>
</ui:UiBinder>
