.pageDetails {
    color: #5d5d5d;
    font-weight: bold;
    line-height: 27px;

    display: inline-block;
    padding: 0px 5px;
    margin: 10px 5px;
}

.button {
    width: 10px;
    height: 16px;
    display: inline-block;
    overflow: hidden;
    margin: 0 4px;

    cursor: pointer;
}

.button:nth-of-type(1), .button:nth-of-type(2) {
    transform: rotateY(180deg);
    -webkit-transform: rotateY(180deg);
}

.button img {
}

.button:hover img {
}

.disabledButton {
    cursor: default;
}
